{
  "extends": ["plugin:@nrwl/nx/react", "eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/no-unknown-property": ["error", { "ignore": ["css"] }]
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
